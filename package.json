{
  "name": "endb",
  "version": "0.22.9",
  "description": "Simple key-value storage with support for multiple backends.",
  "main": "src/index.js",
  "typings": "typings/index.d.ts",
  "files": [
    "src",
    "typings",
    "esm"
  ],
  "exports": {
    ".": [
      {
        "require": "./src/index.js",
        "import": "./esm/index.mjs"
      },
      "./src/index.js"
    ],
    "./esm": "./esm/index.mjs"
  },
  "scripts": {
    "lint": "xo src",
    "lint:fix": "xo src --fix",
    "test": "nyc ava --serial",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coverage:html": "nyc report --reporter=html",
    "test:docker": "docker-compose up --build --abort-on-container-exit --exit-code-from test",
    "posttest:docker": "docker-compose down --rmi local",
    "docs:build": "node docs.js",
    "prepublishOnly": "npm run lint && nyc ava test/index.js"
  },
  "keywords": [
    "endb",
    "key-value",
    "cache",
    "javascript"
  ],
  "license": "MIT",
  "author": "chroventer (https://github.com/chroventer)",
  "homepage": "https://endb.js.org",
  "repository": "chroventer/endb",
  "bugs": "https://github.com/chroventer/endb/issues",
  "dependencies": {
    "buffer-json": "^2.0.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "8.3.4",
    "ava": "^3.7.1",
    "coveralls": "^3.1.0",
    "docma": "^3.2.2",
    "husky": "^4.2.5",
    "mongodb": "^3.5.6",
    "mysql2": "^2.1.0",
    "nyc": "^15.0.1",
    "pg": "^8.0.2",
    "redis": "^3.0.2",
    "sql": "^0.78.0",
    "sqlite3": "^4.1.1",
    "xo": "^0.30.0"
  },
  "ava": {
    "timeout": "30s",
    "files": [
      "!test/functions.js"
    ]
  },
  "xo": {
    "prettier": true,
    "rules": {
      "guard-for-in": 0,
      "no-negated-condition": 0,
      "no-await-in-loop": 0,
      "unicorn/explicit-length-check": 0,
      "unicorn/no-fn-reference-in-iterator": 0
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  }
}
